{
    "version": 2,
    "builds": [
        {
            "src": "/config/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth",
            "dest": "/api/auth.js"
        },
        {
            "src": "/users",
            "dest": "/api/user.js"
        }
    ]
}