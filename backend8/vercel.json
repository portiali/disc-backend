{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth/(.*)",
            "dest": "/api/auth.js"
        },
        {
            "src": "/api/user/(.*)",
            "dest": "/api/user.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}